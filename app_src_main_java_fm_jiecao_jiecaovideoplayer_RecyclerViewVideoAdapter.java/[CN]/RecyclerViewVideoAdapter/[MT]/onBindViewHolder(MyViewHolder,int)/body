{
  Log.i(TAG,"onBindViewHolder [" + holder.jcVideoPlayer.hashCode() + "] position="+ position);
  boolean setUp=holder.jcVideoPlayer.setUp(videoUrls[videoIndexs[position]],JCVideoPlayer.SCREEN_LAYOUT_LIST,videoTitles[videoIndexs[position]]);
  if (setUp) {
    ImageLoader.getInstance().displayImage(videoThumbs[videoIndexs[position]],holder.jcVideoPlayer.thumbImageView);
  }
}
