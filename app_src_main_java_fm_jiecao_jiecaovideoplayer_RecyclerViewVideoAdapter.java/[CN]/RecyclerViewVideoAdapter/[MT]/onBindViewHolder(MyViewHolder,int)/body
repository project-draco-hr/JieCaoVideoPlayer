{
  Log.i(TAG,"onBindViewHolder [" + holder.jcVideoPlayer.hashCode() + "] position="+ position);
  VideoBean video=getItem(position);
  boolean setUp=holder.jcVideoPlayer.setUp(video.url,JCVideoPlayer.SCREEN_LAYOUT_LIST,video.title);
  if (setUp) {
    ImageLoader.getInstance().displayImage(video.thumb,holder.jcVideoPlayer.thumbImageView);
  }
}
