{
  if (isCurrentMediaListener() && (System.currentTimeMillis() - CLICK_QUIT_FULLSCREEN_TIME) < FULL_SCREEN_NORMAL_DELAY)   return false;
  mCurrentState=CURRENT_STATE_NORMAL;
  this.mUrl=url;
  this.mObjects=objects;
  this.mCurrentScreen=screen;
  setUiWitStateAndScreen(CURRENT_STATE_NORMAL);
  return true;
}
