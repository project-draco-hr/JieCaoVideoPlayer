{
  if (!isClickFullscreen) {
    JCMediaManager.intance().mediaPlayer.stop();
    JCMediaManager.intance().listener.onCompletion();
    JCMediaManager.intance().setUuid("");
    JCMediaManager.intance().setUuid("");
    if (mUpdateProgressTimer != null) {
      mUpdateProgressTimer.cancel();
    }
  }
}
