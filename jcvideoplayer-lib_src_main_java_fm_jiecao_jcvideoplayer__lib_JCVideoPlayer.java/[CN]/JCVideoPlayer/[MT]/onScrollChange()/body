{
  if (!isShown()) {
    if (JCVideoPlayerManager.getFirst() == this && url.equals(JCMediaManager.instance().mediaPlayer.getDataSource())) {
      Log.i(TAG,"onScrollChange startWindowTiny [" + this.hashCode() + "] ");
      startWindowTiny();
    }
  }
 else {
    if (JCVideoPlayerManager.getFirst() != this && url.equals(JCMediaManager.instance().mediaPlayer.getDataSource())) {
      Log.i(TAG,"onScrollChange backPress [" + this.hashCode() + "] ");
      backPress();
    }
  }
}
